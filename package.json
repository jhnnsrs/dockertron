{
  "name": "Neural",
  "version": "1.0.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "child_process": "^1.0.2",
    "electron-is-dev": "^1.1.0",
    "electron-react-titlebar": "^0.8.2",
    "python-shell": "^1.0.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-electron-titlebar": "^1.0.9",
    "react-scripts": "3.2.0",
    "reactstrap": "^8.1.1",
    "titlebar": "^1.4.0",
    "windowbar-react": "^1.6.5"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "electron-dev": "concurrently \"set BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run-script build",
    "electron-pack": "electron-builder -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.13",
    "@rescripts/rescript-env": "^0.0.11",
    "concurrently": "^5.0.0",
    "electron": "^15.5.5",
    "electron-builder": "^21.2.0",
    "typescript": "^3.7.2",
    "wait-on": "^3.3.0"
  },
  "author": {
    "name": "Johannes Roos",
    "email": "jhnnsrs@gmail.com",
    "url": "https://johannesroos.de"
  },
  "build": {
    "win": {
      "requestedExecutionLevel": "highestAvailable"
    },
    "appId": "de.johannesroos.neural",
    "productName": "Neural",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "python/**/*"
    ],
    "directories": {
      "buildResources": "public/images"
    }
  },
  "rescripts": [
    "env"
  ]
}
